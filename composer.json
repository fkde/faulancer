{
  "name": "floknapp/faulancer",
  "description": "The Faulancer Framework",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Flo Knapp",
      "email": "office@florianknapp.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Faulancer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Faulancer\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-readline": "*",
    "apix/log": "1.2.1",
    "tflori/orm": "v1.9.0",
    "ramsey/uuid": "4.1.1",
    "nyholm/psr7": "1.4.1",
    "psr/container": "2.0.1",
    "psr/http-client": "1.0.1",
    "beberlei/assert": "v3.3.1",
    "nyholm/psr7-server": "1.0.2",
    "squizlabs/php_codesniffer": "3.6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.10"
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "vendor/bin/phpunit -c phpunit.xml"
  }
}
